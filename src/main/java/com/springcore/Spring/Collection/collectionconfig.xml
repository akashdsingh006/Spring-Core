<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans

http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.springcore.Spring.Collection.Employee" name="employee1">
	
	<property name="name" value="Akash"/>
		<property name="phone">
			<list>
				<value>234796225</value>
				<value>981796225</value>
				<value>321796225</value>
				<value>785213652</value>
			</list>
		</property>
		
		<property name="address">
			<set>
				<value>Kolkata</value>
				<value>Delhi</value>
				<value>Lucknow</value>
			</set>
		</property>
		
		<property name="courses">
			<map>
				<entry key="Java" value="1200" />
				<entry key="C++" value="1800" />
				<entry key="Web Dev" value="9200" />
				
			</map>
		</property>
	</bean>	
	
</beans>